{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "search-client",
    "labels": {
      "app": "search",
      "role": "client"
    }
  },
  "spec": {
    "type": "LoadBalancer",
    "selector": {
      "app": "search",
      "role": "client"
    },
    "ports": [
      {
        "name": "http",
        "port": 9200,
        "protocol": "TCP"
      }
    ]
  }
}
